{
  "name": "@nelsonlaidev/config",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Nelson Lai <me@nelsonlai.dev> (https://github.com/nelsonlaidev/)",
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "check": "turbo lint && turbo typecheck && pnpm format:check && pnpm knip && pnpm lint:spell",
    "clean": "turbo clean && rm -rf .turbo",
    "dev": "turbo dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "knip": "knip",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "lint:spell": "cspell .",
    "prepare": "lefthook install",
    "release": "changeset publish",
    "typecheck": "turbo typecheck",
    "version": "changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@nelsonlaidev/eslint-config": "workspace:*",
    "@nelsonlaidev/prettier-config": "workspace:*",
    "@nelsonlaidev/typescript-config": "workspace:*",
    "@types/node": "^24.10.4",
    "cspell": "^9.6.3",
    "eslint": "^9.39.2",
    "knip": "^5.78.0",
    "lefthook": "^2.0.13",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "turbo": "^2.7.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.26.2",
  "engines": {
    "node": ">=24"
  }
}
